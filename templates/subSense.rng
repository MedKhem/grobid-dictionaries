				</choice>
			</oneOrMore>
		</element>
	</define>

	
	<define name="subSense">
		<element name="subSense">
			<oneOrMore>
				<choice>
					<text/>
					<ref name="pc"/>
					<ref name="lb"/>
					<ref name="def"/>
					<ref name="example"/>
					<ref name="translation"/>
					<ref name="usg"/>
					<ref name="re"/>
					<ref name="xr"/>
					<ref name="num"/>
					<ref name="gramGrp"/>
					<ref name="form"/>
					<ref name="lbl"/>
					<ref name="seg"/>
					<ref name="gloss"/>
					<ref name="etym"/>
					<ref name="note"/>
					<ref name="dictScrap"/>

				</choice>
			</oneOrMore>
		</element>
	</define>
	<define name="body">
		<element name="body">
			<oneOrMore>
				<choice>
					
					<ref name="subSense"></ref>
					
				</choice>
			</oneOrMore>
		</element>
	</define>
	<start>
		<element name="tei">
			<optional>
				<attribute name="xml:space">
					<value>preserve</value>
				</attribute>
			</optional>
			<element name="teiHeader">
				<zeroOrMore>
					<ref name="any_element"/>
				</zeroOrMore>
			</element>
			<element name="text">
			
				<oneOrMore>
					
						<ref name="body"/>
					
				</oneOrMore>
			</element>
		</element>
	</start>
</grammar>
