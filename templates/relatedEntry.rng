<?xml version="1.0" encoding="UTF-8"?>
<grammar ns="" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
    <!-- TODO: TEI namespace -->
    <define name="any_attribute">
        <attribute>
            <anyName/>
        </attribute>
    </define>
    <define name="any_element">
        <element>
            <anyName/>
            <zeroOrMore>
                <ref name="any_attribute"/>
            </zeroOrMore>
            <ref name="any_content"/>
        </element>
    </define>
    <define name="any_content">
        <interleave>
            <zeroOrMore>
                <ref name="any_element"/>
            </zeroOrMore>
            <text/>
        </interleave>
    </define>

    <define name="form">
        <element name="form">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="pc"/>
                    <ref name="lb"/>
                </choice>
            </oneOrMore>
        </element>
    </define>

    <define name="gramGrp">
        <element name="gramGrp">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="pc"/>
                    <ref name="lb"/>
                </choice>
            </oneOrMore>
        </element>
    </define>

    <define name="inflected">
        <element name="inflected">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="pc"/>
                    <ref name="lb"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="variant">
        <element name="variant">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="pc"/>
                    <ref name="lb"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="def">
        <element name="def">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="pc"/>
                    <ref name="lb"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="note">
        <element name="note">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="pc"/>
                    <ref name="lb"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="usg">
        <element name="usg">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="pc"/>
                    <ref name="lb"/>
                </choice>
            </oneOrMore>
        </element>
    </define>



    <define name="dictScrap">
        <element name="dictScrap">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="pc"/>
                    <ref name="lb"/>
                </choice>
            </oneOrMore>
        </element>
    </define>

    <define name="lb">
        <element name="lb">
            <empty/>
        </element>
    </define>
    <define name="pc">
        <element name="pc">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="lb"/>
                </choice>
            </oneOrMore>
        </element>
    </define>


    <define name="re">
        <element name="re">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="pc"/>
                    <ref name="lb"/>
                    <ref name="form"/>
                    <ref name="inflected"/>
                    <ref name="variant"/>
                    <ref name="def"/>
                    <ref name="usg"/>
                    <ref name="gramGrp"/>
                    <ref name="note"/>
                    <ref name="dictScrap"/>

                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="body">
        <element name="body">
            <oneOrMore>
                <choice>

                    <ref name="re"></ref>

                </choice>
            </oneOrMore>
        </element>
    </define>
    <start>
        <element name="tei">
            <optional>
                <attribute name="xml:space">
                    <value>preserve</value>
                </attribute>
            </optional>
            <element name="teiHeader">
                <zeroOrMore>
                    <ref name="any_element"/>
                </zeroOrMore>
            </element>
            <element name="text">

                <oneOrMore>

                    <ref name="body"/>

                </oneOrMore>
            </element>
        </element>
    </start>
</grammar>
